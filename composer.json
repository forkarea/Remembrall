{
  "description": "Remembrall sends email about changes on pages",
  "homepage": "http://github.com/klapuch/remembrall",
  "type": "project",
  "license": "MIT",
  "authors": [
    {
      "name": "Dominik Klapuch",
      "email": "klapuchdominik@gmail.com",
      "homepage": "http://www.facedown.cz"
    }
  ],
  "require": {
    "php": ">=7.0",
    "ext-mbstring": "*",
    "ext-dom": "*",
    "ext-xml": "*",
    "ext-xsl": "*",
    "ext-simplexml": "*",
    "ext-pdo": "*",
    "ext-pdo_pgsql": "*",
    "nette/caching": "^2.5.0",
    "nette/mail": "^2.4.0",
    "tracy/tracy": "^2.4.0",
    "guzzlehttp/guzzle": "^6.2",
    "klapuch/encryption": "@dev",
    "klapuch/storage": "@dev",
    "klapuch/output": "@dev",
    "klapuch/ini": "@dev"
  },
  "require-dev": {
    "nette/tester": "^1.7",
    "mockery/mockery": "^0.9.5"
  },
  "config": {
    "optimize-autoloader": true
  },
  "autoload": {
    "psr-4": {
      "Remembrall\\": [
        "App",
        "Tests"
      ]
    }
  }
}
